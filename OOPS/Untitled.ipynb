    "class father:\n",
    "  def __init__(self, name, age):\n",
    "    self.name = name\n",
    "    self.age = age\n",
    "\n",
    "  def printname(self):\n",
    "    print(self.name, self.age)\n",
    "\n",
    "class son(father):\n",
    "  def __init__(self, name, age):\n",
    "    father.__init__(self, name, age)\n",
    "\n",
    "x = son(\"Varun\", 22)\n",
    "x.printname()"
